#!/usr/bin/env sh

echo 'mas:'
mas upgrade
echo 'brew:'
brew upgrade
brew bundle dump --global --force
echo 'nvim:'
nvim -e -u ~/.config/nvim/init.vim -i NONE -c "PlugUpgrade" -c "PlugUpdate" -c "CocUpdate" -c "qa"
echo 'asdf:'
asdf update
asdf plugin update --all
echo 'rustup:'
rustup update
echo 'tldr:'
tldr --update

echo 'softwareupdate:'
sudo softwareupdate -ir
