        "typos": {
            "sourceName": "typos",
            "command": "typos",
            "args": [
                "%relativepath"
            ],
            "formatLines": 2,
            "formatPattern": [
                "^([^:]+): (.+)\\n  --> (.+):(\\d+):(\\d+)",
                {
                    "line": 4,
                    "column": 5,
                    "message": 2,
                    "security": 1
                }
            ]
        }
    },
    "diagnostic-languageserver.formatFiletypes": {
        "*": [
            "typos"
        ],
        "java": "google-java-format"
    },
    "diagnostic-languageserver.formatters": {
        "google-java-format": {
            "command": "google-java-format",
            "args": [
                "--skip-javadoc-formatting",
                "%file"
            ]
        },
        // "typos": {
        //   "command": "typos",
        //   "args": ["--write-changes", "-"]
        // }
    },
    "languageserver": {
        // "csharp-ls": {
        //   "command": "csharp-ls",
        //   "filetypes": ["cs"],
        //   "rootPatterns": ["*.csproj"]
        // },
        "elm": {
            "command": "elm-language-server",
            "filetypes": [
                "elm"
            ],
            "rootPatterns": [
                "elm.json"
            ]
        },
        // "nix": {
        //   "command": "rnix-lsp",
        //   "filetypes": [
        //     "nix"
        //   ]
        // }
        "roc": {
            "command": "roc_language_server",
            "filetypes": [
                "roc"
            ],
            "rootPatterns": [
                "main.roc"
            ]
        },
        "solidity": {
            "command": "/Users/daviddunn/.asdf/shims/solc",
            "args": [
                "--lsp"
            ],
            "trace.server": "verbose",
            "rootPatterns": [
                "foundry.toml",
                "remappings.txt"
            ],
            "filetypes": [
                "solidity"
            ]
        }
    },
    "eslint.packageManager": "npm",
    "eslint.run": "onSave",
    "eslint.workingDirectories": [
        {
            "mode": "location"
        }
    ],
    "java.compile.nullAnalysis.mode": "automatic",
    "java.format.enabled": false,
    "java.import.generatesMetadataFilesAtProjectRoot": false,
