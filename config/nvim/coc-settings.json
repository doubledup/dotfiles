{
    "diagnostic-languageserver.mergeConfig": true,
    "diagnostic-languageserver.filetypes": {
        "*": [
            "typos"
        ],
        "go": "revive",
    },
    "diagnostic-languageserver.linters": {
        "typos": {
            "sourceName": "typos",
            "command": "typos",
            "args": [
                "--format",
                "sarif",
                "%relativepath"
            ],
            "parseJson": {
                "errorsRoot": "runs[0].results",
                "line": "locations[0].physicalLocation.region.startLine",
                "endLine": "locations[0].physicalLocation.region.endLine",
                "column": "locations[0].physicalLocation.region.startColumn",
                "endColumn": "locations[0].physicalLocation.region.endColumn",
                "message": "${message.markdown}"
            }
        }
    },
    "diagnostic-languageserver.formatFiletypes": {
        "*": [
            "typos"
        ],
        "fish": "fish_indent",
        "java": "google-java-format"
    },
    "diagnostic-languageserver.formatters": {
        "fish_indent": {
            "command": "fish_indent",
            "args": [
                "%file"
            ]
        },
        "google-java-format": {
            "command": "google-java-format_darwin-arm64",
            "args": [
                "%file"
            ]
        },
        // "typos": {
        //   "command": "typos",
        //   "args": ["--write-changes", "-"]
        // }
    },
    "languageserver": {
        // "csharp-ls": {
        //   "command": "csharp-ls",
        //   "filetypes": ["cs"],
        //   "rootPatterns": ["*.csproj"]
        // },
        "elm": {
            "command": "elm-language-server",
            "filetypes": [
                "elm"
            ],
            "rootPatterns": [
                "elm.json"
            ]
        },
        // "nix": {
        //   "command": "rnix-lsp",
        //   "filetypes": [
        //     "nix"
        //   ]
        // }
        "roc": {
            "command": "roc_language_server",
            "filetypes": [
                "roc"
            ],
            "rootPatterns": [
                "main.roc"
            ]
        },
        "solidity": {
            "command": "/Users/daviddunn/.asdf/shims/solc",
            "args": [
                "--lsp"
            ],
            "trace.server": "verbose",
            "rootPatterns": [
                "foundry.toml",
                "remappings.txt"
            ],
            "filetypes": [
                "solidity"
            ]
        },
        "terraform": {
            "command": "terraform-ls",
            "args": [
                "serve"
            ],
            "filetypes": ["terraform"]
        }
    },
    "eslint.packageManager": "npm",
    "eslint.run": "onSave",
    "eslint.workingDirectories": [
        {
            "mode": "location"
        }
    ],
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-21",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-21.jdk/Contents/Home/",
            "default": true
        },
        {
            "name": "JavaSE-11",
            "path": "/Library/Java/JavaVirtualMachines/amazon-corretto-11.jdk/Contents/Home/"
        }
    ],
    "java.format.enabled": false,
    // "java.import.generatesMetadataFilesAtProjectRoot": false,
    // "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Xmx8G -Xms100m -Xlog:disable -javaagent:/Users/daviddunn/.m2/repository/org/projectlombok/lombok/1.18.36/lombok-1.18.36.jar",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Xmx10G -Xms1G -Xlog:disable -XX:+UseStringDeduplication -XX:+OptimizeStringConcat -javaagent:/Users/daviddunn/.m2/repository/org/projectlombok/lombok/1.18.36/lombok-1.18.36.jar",
    // "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -XX:MinHeapFreeRatio=10 -XX:MaxHeapFreeRatio=50 -XX:+UseStringDeduplication -XX:+OptimizeStringConcat -Xmx10G -Xms1G -Xlog:disable -javaagent:/Users/daviddunn/.m2/repository/org/projectlombok/lombok/1.18.36/lombok-1.18.36.jar",
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "tsserver.useLocalTsdk": true,
    "typescript.preferences.quoteStyle": "double",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifierEnding": "minimal",
    "rust-analyzer.diagnostics.experimental.enable": false,
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "snippets.ultisnips.pythonPrompt": false,
    "yaml.customTags": [
        "!And sequence",
        "!Base64 scalar",
        "!Cidr scalar",
        "!Condition scalar",
        "!Equals sequence",
        "!FindInMap sequence",
        "!GetAZs scalar",
        "!GetAtt scalar",
        "!GetAtt sequence",
        "!If sequence",
        "!ImportValue scalar",
        "!Join sequence",
        "!Not sequence",
        "!Or sequence",
        "!Ref scalar",
        "!Select sequence",
        "!Split sequence",
        "!Sub scalar",
        "!Transform mapping"
    ],
    "zig.enabled": true,
    "zig.startUpMessage": true,
    "zig.path": "/opt/homebrew/bin/zls",
    "zig.debugLog": false,
}
